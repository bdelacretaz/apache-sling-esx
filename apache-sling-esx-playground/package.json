{
  "name": "apache-sling-esx-playground",
  "version": "1.0.0",
  "description": "Apache Sling ESX Playground",
  "main": "index.js",
  "scripts": {
    "dev": "cd jcr_root/apps/esx-examples && repo put -f",
    "livereload": "live-reload --port 9091 jcr_root/apps/esx-examples/",
    "watcher": "watch  'npm run dev' jcr_root/apps/esx-examples",
    "dev:watch": "parallelshell \"npm run watcher\"  \"npm run livereload\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/labertasch/apache-sling-esx-playground.git"
  },
  "author": "Senol Tas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/labertasch/apache-sling-esx-playground/issues"
  },
  "homepage": "https://github.com/labertasch/apache-sling-esx-playground#readme",
  "devDependencies": {
    "less": "^2.7.2",
    "live-reload": "^1.1.0",
    "parallelshell": "^2.0.0",
    "watch": "^1.0.2"
  }
}
